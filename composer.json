{
    "name": "mralston/lockout",
    "description": "Locks user accounts after a set number of failed login attempts.",
    "keywords": [
        "mralston",
        "laravel",
        "lockout"
    ],
    "homepage": "https://github.com/mralston/lockout",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Ralston",
            "email": "matt@mralston.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/auth": "^8.68",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/http": "^8.68",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mralston\\Lockout\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mralston\\Lockout\\Providers\\LockoutServiceProvider",
                "Mralston\\Lockout\\Providers\\EventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
